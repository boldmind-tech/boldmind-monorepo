{
  "name": "boldmind-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "rm -rf node_modules && rm -rf **/node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\" --ignore-path .gitignore",
    "dev:live": "turbo run dev --filter=@boldmind/boldmind-hub --filter=@boldmind/amebogist --filter=@boldmind/educenter --filter=@boldmind/planai-receptionist",
    "dev:building": "turbo run dev --filter=@boldmind/boldmind-os --filter=@boldmind/social-factory",
    "dev:new": "turbo run dev --filter=@boldmind/naija-fither --filter=@boldmind/emailscraper-pro --filter=@boldmind/safe-ai",
    "dev:planai-landing": "turbo run dev --filter=./APPS/WEB_APPS/PLANAI_SUITE/planai-landing",
    "dev:planai-receptionist": "turbo run dev --filter=./APPS/WEB_APPS/PLANAI_SUITE/receptionist",
    "dev:planai-credibility-hubs": "turbo run dev --filter=./APPS/WEB_APPS/PLANAI_SUITE/credibility-hubs",
    "dev:planai-business-planning": "turbo run dev --filter=./APPS/WEB_APPS/PLANAI_SUITE/business-planning",
    "dev:afrohustle-os": "turbo run dev --filter=./APPS/CONCEPT_APPS/afrohustle-os",
    "dev:naijagig-matcher": "turbo run dev --filter=./APPS/CONCEPT_APPS/naijagig-matcher",
    "dev:kolo-ai": "turbo run dev --filter=./APPS/CONCEPT_APPS/kolo-ai",
    "dev:planai": "turbo run dev --filter=./APPS/WEB_APPS/PLANAI_SUITE/*",
    "dev:concepts": "turbo run dev --filter=./APPS/CONCEPT_APPS/*",
    "dev:all": "turbo run dev --parallel",
    "build:shared": "turbo run build --filter=./PACKAGES/*",
    "build:live": "turbo run build --filter=@boldmind/boldmind-hub --filter=@boldmind/amebogist --filter=@boldmind/educenter --filter=@boldmind/planai-receptionist",
    "build:selected": "turbo run build --filter=@boldmind/boldmind-hub --filter=@boldmind/amebogist --filter=@boldmind/educenter --filter=@boldmind/boldmind-os --filter=@boldmind/naija-fither --filter=@boldmind/emailscraper-pro --filter=@boldmind/safe-ai --filter=@boldmind/social-factory",
    "install:all": "pnpm install && pnpm -r install",
    "update:deps": "pnpm update --latest --recursive --workspace-root",
    "check:deps": "pnpm -r exec npm outdated",
    "audit:deps": "pnpm audit --recursive",
    "fix:deps": "pnpm dedupe",
    "deploy:live": "turbo run deploy --filter=@boldmind/boldmind-hub --filter=@boldmind/amebogist --filter=@boldmind/educenter --filter=@boldmind/planai-receptionist",
    "deploy:planai": "turbo run deploy --filter=@boldmind/planai-*",
    "stats": "turbo run stats",
    "graph": "turbo run build --graph",
    "type-check": "turbo run type-check",
    "vercel:amebogist": "turbo run build --filter=@boldmind/ui --filter=@boldmind/utils --filter=@boldmind/api-client --filter=@boldmind/amebogist",
    "vercel:planai-planai-landing": "turbo run build --filter=@boldmind/ui --filter=@boldmind/utils --filter=@boldmind/ai --filter=@boldmind/planai-receptionist",
    "vercel-build:boldmind-hub": "pnpm install && pnpm run vercel:boldmind-hub",
    "vercel-build:educenter": "pnpm install && pnpm run vercel:educenter",
    "vercel-build:amebogist": "pnpm install && pnpm run vercel:amebogist",
    "vercel:boldmind-hub": "turbo run build --filter=...@boldmind/boldmind-hub",
  "vercel:educenter": "turbo run build --filter=...@boldmind/educenter",
  "vercel:planai-landing": "turbo run build --filter=...@boldmind/planai-landing",
    "vercel-build:planai-receptionist": "pnpm install && pnpm run vercel:planai-receptionist",
  "vercel-build:planai-landing": "pnpm install && pnpm run vercel:planai-landing"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "typescript": "^5.3.3",
    "turbo": "^1.13.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.16",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/swagger": "^7.3.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "^14.2.0",
    "framer-motion": "^11.0.5",
    "lucide-react": "^0.309.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "axios": "^1.6.7",
    "@tanstack/react-query": "^5.17.0",
    "zustand": "^4.4.7",
    "react-hook-form": "^7.49.3",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "next": "^14.2.0",
      "typescript": "^5.3.3",
      "@types/react": "^18.2.61",
      "@types/react-dom": "^18.2.19",
      "@types/node": "^20.11.5"
    }
  }
}
