{
  "name": "boldmind-hub",
  "framework": "nextjs",
  "buildCommand": "cd ../.. && pnpm install && pnpm run build:shared && pnpm turbo run build --filter=@boldmind/boldmind-hub",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install",
  "devCommand": "cd ../.. && pnpm turbo run dev --filter=@boldmind/boldmind-hub",
  "regions": ["fra1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.boldmind.ng",
    "NEXT_PUBLIC_SITE_URL": "https://boldmind.ng"
  },
  "alias": ["boldmind.ng", "www.boldmind.ng"]
}