{
  "name": "boldmind-so",
  "framework": "nextjs",
  "buildCommand": "cd ../.. && pnpm install && pnpm turbo run build --filter=boldmind-so",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install",
  "devCommand": "cd ../.. && pnpm turbo run dev --filter=boldmind-hub",
  "regions": ["fra1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.boldmind.ng",
    "NEXT_PUBLIC_SITE_URL": "https://os.boldmind.ng"
  },
  "alias": ["os.boldmind.ng", "www.os.boldmind.ng"]
}