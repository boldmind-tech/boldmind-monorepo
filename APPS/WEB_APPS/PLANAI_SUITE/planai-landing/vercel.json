{
  "name": "planai-landing",
  "framework": "nextjs",
  "buildCommand": "cd ../../../.. && pnpm run vercel-build:planai-landing",
  "outputDirectory": ".next",
  "installCommand": "cd ../../../.. && pnpm install",
  "devCommand": "cd ../../../.. && pnpm run dev --filter=@boldmind/planai-landing",
  "regions": ["fra1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.planai.boldmind.ng",
    "NEXT_PUBLIC_SITE_URL": "https://planai.boldmind.ng"
  },
  "alias": ["planai.boldmind.ng", "www.planai.boldmind.ng"]
}