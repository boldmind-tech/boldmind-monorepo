{
  "name": "afrohustle-os",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "regions": ["fra1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.boldmind.ng",
    "NEXT_PUBLIC_APP_URL": "https://hustle.boldmind.ng"
  },
  "alias": ["hustle.boldmind.ng"]
}